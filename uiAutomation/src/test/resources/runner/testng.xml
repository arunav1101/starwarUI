<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Addressbook Full Regression Test Run">

    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
        <listener class-name="com.addressbook.listeners.CustomListeners"></listener>
    </listeners>

    <test thread-count="1" name="Login Test">
        <classes>
            <class name="com.addressbook.testcases.LoginTest"/>
        </classes>
    </test>
    <test thread-count="1" name="Add Address Test">
        <classes>
            <class name="com.addressbook.testcases.starwars"/>
        </classes>
    </test>

</suite>
